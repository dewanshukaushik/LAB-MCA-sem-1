#include <stdio.h>

void main()
{
   int a[7] = {6,2,7,5,9,10,5};
    int i, j=6;
    for(i=0; j >= 0; i++)
    {
        if(i<7)
        {
            printf("%d,",a[i]);
            continue;
        }
        if (i == 7)
        {
            printf("\n");
        }
    printf("%d,",a[j]);
    j--;
}
}










//* 
for given number

#include <stdio.h>
#include <math.h>

int main()
{
   int count=0,p=0;
   int temp,num,n,t;
   printf("Enter a number ");
   scanf("%d",&num);
    temp = num;
    while(temp>0)
    {
       count++;
       temp=temp/10;
    }
    n=num;
    while(n>0)
        {
            t=n%10;
            p=p+pow(t,count);
            n=n/10;
        }
        if (p == num)
        {
            printf("armstrong");
        }
         else
         {
             printf("not armstrong");
         }
        
 return (0);
}
