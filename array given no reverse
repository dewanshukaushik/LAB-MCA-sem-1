#include <stdio.h>

void main()
{
   int a[7] = {6,2,7,5,9,10,5};
    int i, j=6;
    for(i=0; j >= 0; i++)
    {
        if(i<7)
        {
            printf("%d,",a[i]);
            continue;
        }
        if (i == 7)
        {
            printf("\n");
        }
    printf("%d,",a[j]);
    j--;
}
}
