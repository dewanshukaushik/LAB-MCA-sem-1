
#include <stdio.h>
#include <math.h>

int main() {
   int number,i;
   int flag=0;
   printf("enter a number ");
   scanf("%d",&number);
   for(i=2;i<=sqrt(number);i++)
   {
       if(number%i==0)
       {
           flag=1;
           break;
           
       }
   }
   if(flag==1)
   {
       printf("not prime");
   }
   else
   printf("prime");
   
 
    return 0;
}
